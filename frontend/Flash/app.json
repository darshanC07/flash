{
  "expo": {
    "name": "Flash",
    "slug": "Flash",
    "scheme": "flashapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.darshanc07.Flash"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.darshanc07.Flash",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "flashapp",
            "host": "*"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "91fc3602-a893-47bd-bdb5-9a35fe484cb1"
      }
    }
  }
}
